     code : |1-
      
      // SQL colorization
      SQL is cSQLColoring()
      
      // XML colorization
      XML is cXMLColoring()
      
      // HTML coloring
      HTML is cHTMLColoring()
      
      // Defines the output mode for the colorizations: RTF mode (in an RTF edit control)
      SQL:SetOutputMode(cSQLColoring::OUT_RTF)
      XML:SetOutputMode(cSQLColoring::OUT_RTF)
      HTML:SetOutputMode(cSQLColoring::OUT_RTF)
   -
     code : |1-
      
      // See the 'Whenever modified' code of the EDT_SQLCODE, EDT_XMLCODE and EDT_HTMLCODE edit controls
      
      // Defines default SQL/XML code
      SQL:SetCode(RTFToText(EDT_SQLCODE..InitialValue))
      XML:SetCode(RTFToText(EDT_XMLCODE..InitialValue))
      HTML:SetCode(RTFToText(EDT_HTMLCODE..InitialValue))
      
      // Performs the coloring
      EDT_SQLCODE = SQL:Coloring()
      EDT_XMLCODE = XML:Coloring()
      EDT_HTMLCODE = HTML:Coloring()
