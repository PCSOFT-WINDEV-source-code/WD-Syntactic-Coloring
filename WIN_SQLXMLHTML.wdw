               END
               
               // Defines the SQL code
               SQL:SetCode(bufCode)
               
               EDT_SQLCODE..DisplayEnabled = False
               
               // Performs the coloring
               EDT_SQLCODE = SQL:Coloring()
               
               // Repositions the input cursor
               EDT_SQLCODE..Cursor = nCursor + (bCRManagement ? Max(0,(StringCount(bufCode, CR) - 1)) ELSE 0)
               
