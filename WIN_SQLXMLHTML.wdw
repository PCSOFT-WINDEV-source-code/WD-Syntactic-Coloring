    name : BTN_Open
    identifier : 0x286d0a700276bde6
    internal_properties : 
    properties :
     x : 253
     y : 268
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         sFile 	is string
         bufContent	is string
         
         SWITCH TAB_Tab1
         
         	// SQL
         	CASE 1
         		
         		// Opens the file picker
         		sFile = fSelect("", "", "Select a file...", "SQL code (*.sql)"+TAB+"*.SQL"+CR+"All files (*.*)"+TAB+"*.*", "SQL", fselOpen+fselExist)
         		
         	// XML
         	CASE 2
         		
         		// Opens the file picker
         		sFile = fSelect("", "", "Select a file...", "XML documents (*.xml)"+TAB+"*.XML"+CR+"All files (*.*)"+TAB+"*.*", "XML", fselOpen+fselExist)
         		
         END
         
         IF sFile <> "" THEN
         	
         	HourGlass(True)
         	
         	// Loads the content of the file
         	bufContent = fLoadText(sFile)
         	
         	SWITCH TAB_Tab1
         	
         		// SQL
         		CASE 1
         			
         			// Loads the code
         			SQL:SetCode(bufContent)
         			
         			// Performs the coloring
         			EDT_SQLCODE = SQL:Coloring()
         			
         		// XML
         		CASE 2
         			
         			// Loads the code
         			XML:SetCode(bufContent)
         			
         			// Performs the coloring
         			EDT_XMLCODE = XML:Coloring()			
         	END
         	
         	HourGlass(False)
         END
        type : 18
